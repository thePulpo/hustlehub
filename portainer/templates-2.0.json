{
  "version": "2",
  "templates": [
    {
      "type": 1,
      "title": "Registry",
      "description": "Docker image registry",
      "categories": [
        "docker"
      ],
      "platform": "linux",
      "logo": "https://github.com/thePulpo/hustlehub/raw/templates/portainer/logos/docker.png",
      "image": "registry:latest",
      "ports": [
        "5000/tcp"
      ],
      "volumes": [
        {
          "container": "/var/lib/registry"
        }
      ]
    },
    {
      "type": 1,
      "title": "Keycloak",
      "description": "Identity and Access Management solution",
      "categories": [
        "docker"
      ],
      "platform": "linux",
      "logo": "https://github.com/thePulpo/hustlehub/raw/templates/portainer/logos/keycloak.png",
      "image": "jboss/keycloak:15.0.2",
      "ports": [
        "8080:8080/tcp"
      ],
      "name": "test",
      "env": [
        {
          "name": "KEYCLOAK_USER",
          "label": "Keycloak Admin Username",
          "default": "admin",
          "type": "string"
        },
        {
          "name": "KEYCLOAK_PASSWORD",
          "label": "Keycloak Admin Password",
          "default": "admin",
          "type": "password"
        }
      ]
    }
  ]
}